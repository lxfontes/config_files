AddKeysToAgent yes

Host *
  ServerAliveInterval 10
  ServerAliveCountMax 9999

Host github.com
	User git
	IdentityFile ~/.ssh/id_github

Host bitbucket.org
	User git
	IdentityFile ~/.ssh/id_github

Host localdev
  HostName 127.0.0.1
  Port 8222
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
