AddKeysToAgent yes

Host workstation
  HostName 10.124.59.82
  ForwardAgent yes
  User lxfontes

Host github.com
	User git
	IdentityFile ~/.ssh/github_rsa

Host bitbucket.org
	User git
	IdentityFile ~/.ssh/github_rsa

Host github.internal.digitalocean.com
	User git
	IdentityFile ~/.ssh/do_rsa

Host jump
  HostName jump.internal.digitalocean.com
  ForwardAgent yes
  User lfontes
	IdentityFile ~/.ssh/do_rsa

Host stage2-jump
  HostName stage2-jump.internal.digitalocean.com
  ForwardAgent yes
  User lfontes
	IdentityFile ~/.ssh/do_rsa

Host *.s2r1.internal.digitalocean.com
  User lfontes
  ServerAliveInterval 15
  ProxyCommand ssh stage2-jump -W %h:%p 2>/dev/null

Host *.digitalocean.com
  User lfontes
  ServerAliveInterval 15
  ProxyCommand ssh jump -W %h:%p 2>/dev/null
