AddKeysToAgent yes

Host *
  ServerAliveInterval 10

Host git.herokai.com
	User git
	IdentityFile ~/.ssh/id_rsa

Host github.com
	User git
	IdentityFile ~/.ssh/github_rsa

Host bitbucket.org
	User git
	IdentityFile ~/.ssh/github_rsa

Host localdev
  HostName 127.0.0.1
  Port 8222
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
